<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ads="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/cv_tab_upcoming_journey" android:background="@color/lighter_grey" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="7dp">
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_tab_upcoming_journey_item_ticket" android:paddingTop="20dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingHorizontal="20dp">
            <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="4">
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/tv_tab_upcoming_journey_train" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Train express gandhidham" android:fontFamily="@font/open_sans_bold"/>
                <TextView android:textSize="12dp" android:textColor="@color/darkGrey" android:id="@+id/tv_tab_upcoming_journey_train_num" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="(Number)" android:fontFamily="@font/open_sans_bold"/>
            </LinearLayout>
            <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/tv_tab_upcoming_journey_pnr" android:paddingRight="4dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="12345678911" android:fontFamily="@font/open_sans_bold"/>
            <LinearLayout android:orientation="vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="3">
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/tv_tab_upcoming_journey_date" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Tue,22 Sep" android:fontFamily="@font/open_sans_bold"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/tv_timestamp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="10:10:30" android:fontFamily="@font/open_sans_bold"/>
            </LinearLayout>
            <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/tv_tab_upcoming_journey_from" android:paddingLeft="5dp" android:layout_width="0dp" android:layout_height="wrap_content" android:text="NDLS" android:layout_weight="2" android:fontFamily="@font/open_sans_bold"/>
            <TextView android:textSize="12dp" android:textColor="@color/dark" android:gravity="right" android:id="@+id/tv_tab_upcoming_journey_to" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/To" android:layout_weight="2" android:fontFamily="@font/open_sans_bold"/>
        </LinearLayout>
        <LinearLayout android:paddingTop="10dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingHorizontal="20dp">
            <TextView android:textSize="12dp" android:textColor="@color/dark" android:id="@+id/visit_booking_history" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/please_visit_booking_history" android:fontFamily="@font/open_sans_bold"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="0dp" android:paddingHorizontal="20dp" android:paddingVertical="10dp">
            <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_tab_delhi_metro" android:background="@color/light_grey" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginRight="20dp" android:text="@string/delhi_metro" android:layout_weight="2" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
            <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_tab_upcoming_journey_hotel" android:background="@color/light_grey" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginRight="20dp" android:text="@string/Hotel" android:layout_weight="2" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
            <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_tab_upcoming_journey_retroom" android:background="@color/light_grey" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginRight="20dp" android:text="@string/Retiring_Room" android:layout_weight="3" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
            <TextView android:textColor="@color/white" android:gravity="center" android:layout_gravity="end" android:id="@+id/tv_bus" android:background="@color/dark" android:layout_width="0dp" android:layout_height="match_parent" android:text="@string/bus" android:layout_weight="2" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:background="#f5f7fa" android:layout_width="match_parent" android:layout_height="wrap_content" android:paddingHorizontal="20dp" android:paddingVertical="10dp">
            <TextView android:textColor="@color/white" android:gravity="center" android:id="@+id/tv_tab_upcoming_journey_mot" android:background="@color/light_grey" android:layout_width="0dp" android:layout_height="match_parent" android:layout_marginRight="10dp" android:text="@string/orderfood_e_Catering" android:layout_weight="3" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
            <TextView android:textColor="@color/white" android:gravity="center" android:layout_gravity="end" android:id="@+id/orderfood_Pantry_Car" android:background="@color/dark" android:visibility="visible" android:layout_width="0dp" android:layout_height="match_parent" android:text="@string/orderfood_Pantry_Car" android:layout_weight="3" android:fontFamily="@font/open_sans_bold" android:paddingHorizontal="5dp" android:paddingVertical="5dp"/>
        </LinearLayout>
    </LinearLayout>
    <View android:id="@+id/viewmargin" android:background="@color/lighter_grey" android:layout_width="match_parent" android:layout_height="7dp"/>
</androidx.cardview.widget.CardView>
