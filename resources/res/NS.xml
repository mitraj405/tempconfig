<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ads="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:id="@+id/transaction" android:layout_width="match_parent" android:layout_height="wrap_content">
        <RelativeLayout android:id="@+id/last_transation_detail_ll" android:background="@color/white" android:padding="15dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/last_transaction" android:tint="@color/dark"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/last_Transaction" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <RelativeLayout android:id="@+id/rl_mybooking" android:background="@color/white" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/my_bookings_grey"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/my_Bookings" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <RelativeLayout android:gravity="center_vertical" android:id="@+id/rl_failedtransaction" android:background="@color/white" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/failed_transaction"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/failed_history" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <RelativeLayout android:gravity="center_vertical" android:id="@+id/rl_cancelledticket" android:background="@color/white" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/cancelled_ticket"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/tkt_cancellation_history" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <RelativeLayout android:gravity="center_vertical" android:id="@+id/rl_tdrhistory" android:background="@color/white" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/tdr_history"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/tdr_history" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
        <RelativeLayout android:gravity="center_vertical" android:id="@+id/rl_ticketrefund" android:background="@color/white" android:padding="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <ImageView android:layout_width="15dp" android:layout_height="15dp" android:src="@drawable/spinner_upwards" android:tint="@color/dark" android:layout_alignParentRight="true" android:rotation="90"/>
            <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
                <ImageView android:padding="2dp" android:layout_width="25dp" android:layout_height="25dp" android:src="@drawable/ticket_refund"/>
                <TextView android:textSize="12dp" android:textColor="@color/dark" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:text="@string/refund_history" android:fontFamily="@font/open_sans"/>
            </LinearLayout>
        </RelativeLayout>
        <View android:background="@color/lightdark" android:layout_width="match_parent" android:layout_height="0.5dp"/>
    </LinearLayout>
    <com.google.android.gms.ads.admanager.AdManagerAdView android:layout_gravity="center" android:id="@+id/my_transaction_bottom" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:layout_below="@+id/transaction" android:layout_centerHorizontal="true" ads:adSizes="MEDIUM_RECTANGLE,LARGE_BANNER,BANNER" ads:adUnitId="@string/my_transaction_bottom"/>
</RelativeLayout>
