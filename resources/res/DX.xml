<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:ads="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:id="@+id/tkt_layout_id" android:layout_width="match_parent" android:layout_height="wrap_content">
    <LinearLayout android:orientation="horizontal" android:id="@+id/ticket_info_layout" android:background="@color/white_90_opa" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout android:orientation="horizontal" android:id="@+id/pnr_details_layout" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="10dp" android:weightSum="10">
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/ac_from" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_account_start" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="10 May 18" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/to_colam" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_account_end" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="10 May 2018" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:background="@color/white_90_opa" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:weightSum="10">
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/pts_expired" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_annual_expired_point" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0000" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/on" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_expiry_point" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="11 may 2020" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:background="@color/white_90_opa" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:weightSum="10">
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Travel_Pts" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_annual_travel_point" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0000" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Partner_Pts" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_annual_partner_point" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0000" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:background="@color/white_90_opa" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="0dp" android:weightSum="10">
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Purchase_Pts" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_annual_purchase_point" android:paddingRight="5dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0000" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="5">
                <TextView android:textSize="14dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/Pts_Redeemed" fontFamily="fonts/Roboto-Regular.ttf"/>
                <TextView android:textSize="14dp" android:textColor="@color/black_90_opa" android:id="@+id/ly_redeem_point" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0000" fontFamily="fonts/Roboto-Medium.ttf"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <View android:background="@color/light_gray" android:layout_width="match_parent" android:layout_height="10dp" android:layout_marginTop="10dp"/>
</LinearLayout>
